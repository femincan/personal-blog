    "astro": "^3.4.0",
    "astro-robots-txt": "^1.0.0",
    "dayjs": "^1.11.10",
    "highlight.js": "^11.9.0",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
